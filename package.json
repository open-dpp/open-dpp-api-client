{
  "name": "@open-dpp/api-client",
  "version": "1.3.0",
  "description": "A client for my API",
  "repository": {
    "url": "https://github.com/open-dpp/open-dpp-api-client"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsc && rollup -c",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "lint": "eslint \"{src,tests}/**/*.ts\"",
    "lint:fix": "eslint \"{src,tests}/**/*.ts\" --fix",
    "check-ts": "tsc --noEmit",
    "test": "jest",
    "patch": "git fetch --tags && npm version patch && git push --follow-tags",
    "minor": "git fetch --tags && npm version minor && git push --follow-tags",
    "major": "git fetch --tags && npm version major && git push --follow-tags"
  },
  "keywords": [],
  "author": "open-dpp",
  "license": "ISC",
  "type": "module",
  "dependencies": {
    "axios": "^1.7.9"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.20.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.9",
    "@typescript-eslint/eslint-plugin": "^8.24.0",
    "@typescript-eslint/parser": "^8.24.0",
    "eslint": "^9.20.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-jest": "^28.11.0",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "msw": "^2.7.0",
    "prettier": "^3.5.0",
    "rollup": "^4.34.8",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.3"
  }
}
